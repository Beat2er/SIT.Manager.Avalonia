<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--Play Page / View-->
	<system:String x:Key="AddressBoxTitle">Address</system:String>
	<system:String x:Key="UsernameBoxTitle">Username</system:String>
	<system:String x:Key="PasswordBoxTitle">Password</system:String>
